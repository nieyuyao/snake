{
	"env": {
		"browser": true,
		"es6": true
	},
	"parser": "babel-eslint",
	"rules": {
		"indent": ["warn", "tab"],
		"no-tabs": "off",
		"no-trailing-spaces": "off",
		"comma-dangle": "off",
		"semi": "off",
		"object-curly-spacing": "off",
		"lines-between-class-members": "off",
		"quote-props": "off",
		"object-shorthand": "off",
		"arrow-parens": "off",
		"no-undef": "off",
		"no-plusplus": "off",
		"eol-last": "off",
		"spaced-comment": "off",
		"object-curly-newline": "off",
		"no-continue": "off",
		"max-len": "off",
		"eqeqeq": "off",
		"no-unsafe-negation": "off",
		"quotes": "off",
		"operator-assignment": "off",
		"no-unused-vars": "off",
		"prefer-destructuring": "off",
		"no-else-return": "off",
		"no-useless-return": "off",
		"brace-style": "off",
		"no-multi-spaces": "off",
		"prefer-template": "off",
		"no-useless-rename": "off",
		"sort-imports": "off",
		"no-restricted-syntax": "off",
		"guard-for-in": "off",
		"no-shadow": "off",
		"no-param-reassign": "off",
		"operator-linebreak": "off",
		"prefer-const": "off",
		"new-cap": "off",
		"class-methods-use-this": "off"
	}
}